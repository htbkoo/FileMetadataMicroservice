{
  "name": "url_shortener_microservice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "node ./dist/bin/www",
    "buildStart": "npm run build && npm run start",
    "debug": "set DEBUG=express:* & node %NODE_DEBUG_OPTION% ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bson": "^1.0.4",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "mongodb": "^2.2.28",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.0.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "mongo-mock": "^2.4.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.4",
    "sinon-test": "^1.0.2",
    "string-format": "^0.5.0"
  }
}
